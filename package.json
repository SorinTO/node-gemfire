{
  "name": "gemfire",
  "version": "0.0.2",
  "description": "Pivotal Gemfire Node package proof-of-concept",
  "main": "./lib",
  "files": [
    "lib/**/*.js",
    "src",
    "binding.gyp"
  ],
  "author": "Pivotal Software, Inc.",
  "contributors": [
    {
      "name": "Grant Hutchins",
      "url": "https://github.com/nertzy"
    },
    {
      "name": "Taavo Smith",
      "url": "http://github.com/taavo"
    },
    {
      "name": "Michael Schubert",
      "url": "http://github.com/schubert"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:pivotal/nodejs-gemfire-poc.git"
  },
  "binary": {
    "module_name": "gemfire",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path": "./node-{module_name}/{version}/",
    "host": "https://download.pivotal.com.s3.amazonaws.com"
  },
  "dependencies": {
    "node-pre-gyp": "0.5.x",
    "nan": "~1.3.0"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "grunt test"
  },
  "devDependencies": {
    "aws-sdk": "^2.0.15",
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-shell": "^1.0.0",
    "jasmine-node": "^1.14.5",
    "lodash": "^2.4.1",
    "microtime": "^0.6.0",
    "random-string": "^0.1.1"
  }
}
